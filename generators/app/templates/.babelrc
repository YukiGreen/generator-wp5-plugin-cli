{
"presets": ["@babel/preset-typescript", "@vue/cli-plugin-babel/preset"],
    "plugins": [
      ["@babel/plugin-syntax-dynamic-import"],
      [
        "component",
        {
          // 组件库的名字，需要和 package.json 里的 name 相同
          "libraryName": "names",
          // 存放组件的文件夹，如果不想配置此项，默认文件夹的名字为 lib
          "libDir": "lib",
          "styleLibraryName":"theme-chalk"
        }
      ]
    ]
  }
